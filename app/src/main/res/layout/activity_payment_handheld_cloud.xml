<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <com.app.tanklib.view.BsoftActionBar
        android:id="@+id/actionbar"
        style="@style/BsoftActionBar">
    </com.app.tanklib.view.BsoftActionBar>

    <TextView
        android:id="@+id/tv_pending_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/dialog_select"
        android:paddingLeft="20dp"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:text="@string/cloud_pending_item"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_16"/>

    <View
        style="@style/line_inner"/>

    <TextView
        android:id="@+id/tv_paid_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/dialog_select"
        android:paddingLeft="20dp"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:text="@string/cloud_paid_item"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_16"/>

    <View
        style="@style/line_outer"/>
</LinearLayout>