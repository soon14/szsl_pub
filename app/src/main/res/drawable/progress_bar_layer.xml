<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="5.0dip">
			</corners>
			<gradient android:startColor="#dedede" android:endColor="#dedede"
				android:angle="270.0" android:centerY="0.75" android:centerColor="#dedede">
			</gradient>
		</shape>
	</item>
	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<corners android:radius="5.0dip">
				</corners>
				<gradient android:startColor="#9e9e9e" android:endColor="#9e9e9e"
					android:angle="270.0" android:centerY="0.75" android:centerColor="#9e9e9e">
				</gradient>
			</shape>
		</clip>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="5.0dip">
				</corners>
				<gradient android:startColor="#9e9e9e" android:endColor="#9e9e9e"
					android:angle="270.0" android:centerY="0.75" android:centerColor="#9e9e9e">
				</gradient>
			</shape>
		</clip>
	</item>
</layer-list>
